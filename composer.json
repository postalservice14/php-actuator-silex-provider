{
    "name": "postalservice14/php-actuator-silex-provider",
    "type": "library",
    "description": "Silex Provider for php-actuator",
    "keywords": [
        "postalservice14",
        "php-actuator-silex-provider"
    ],
    "homepage": "https://github.com/postalservice14/php-actuator-silex-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "John Kelly",
            "email": "johnmkelly86@gmail.com",
            "homepage": "http://www.github.com/postalservice14",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "silex/silex": "^1.3",
        "postalservice14/php-actuator": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Actuator\\Silex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actuator\\Silex\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
